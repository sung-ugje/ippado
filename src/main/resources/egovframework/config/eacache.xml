<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="https://www.ehcache.org/ehcache.xsd"
	updateCheck="false">

	<defaultCache eternal="false" maxElementsInMemory="50"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="100"
		timeToLiveSeconds="200" memoryStoreEvictionPolicy="LRU" />

	<cache name="globalMenuCache" eternal="false"
		maxElementsInMemory="100" overflowToDisk="false"
		diskPersistent="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LRU" />

</ehcache>